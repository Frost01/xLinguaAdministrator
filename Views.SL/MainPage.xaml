<UserControl x:Class="Views.SL.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:viewmodels="clr-namespace:Views.SL.ViewModels"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400">
    <UserControl.DataContext>
        <viewmodels:MainPageViewModel></viewmodels:MainPageViewModel>
    </UserControl.DataContext>

    <Grid x:Name="LayoutRoot" Background="White">
        <ListBox ItemsSource="{Binding Path=Languages}" DisplayMemberPath="Text"/>
    </Grid>
</UserControl>
